  "version": "1.0.0",
  "scripts": {
    "dev": "concurrently -n expo,convex \"bun run dev:expo\" \"bun run dev:convex\"",
    "dev:expo": "expo start -c",
    "dev:convex": "bunx convex dev",
    "dev:web": "expo start -c --web",
    "dev:android": "expo start -c --android",
    "android": "expo start -c --android",
    "ios": "expo start -c --ios",
    "web": "expo start -c --web",
    "clean": "rm -rf .expo node_modules",
    "postinstall": "bunx tailwindcss -i ./global.css -o ./node_modules/.cache/nativewind/global.css"
  },
  "dependencies": {
