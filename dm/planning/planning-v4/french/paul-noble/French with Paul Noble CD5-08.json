    "timestamp" : "00:06.420-00:08.320"
  },
  {
    "text" : "To stay",
    "timestamp" : "00:08.320-00:09.780"
  },
  {
    "text" : "Rester",
    "timestamp" : "00:09.780-00:12.080"
  },
  {
    "text" : "To desire",
    "timestamp" : "00:12.080-00:13.840"
  },
  {
    "text" : "Désirer",
    "timestamp" : "00:13.840-00:15.840"
  },
  {
    "text" : "To pay",
