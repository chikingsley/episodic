    "timestamp" : "00:00.001-00:14.140"
  },
  {
    "timestamp" : "00:14.140-00:16.600",
    "text" : "Mon ami Pierrot"
  },
  {
    "timestamp" : "00:16.600-00:19.720",
    "text" : "Prête-moi ta plume"
  },
  {
    "text" : "Pour écrire un mot",
    "timestamp" : "00:19.720-00:22.300"
  },
  {
    "timestamp" : "00:22.300-00:25.360",
    "text" : "Ma chandelle est morte"
  },
  {
    "text" : "Je n'ai plus de feu",
    "timestamp" : "00:25.360-00:27.940"
  },
  {
    "timestamp" : "00:27.940-00:30.980",
    "text" : "Ouvre-moi ta porte"
  },
  {
    "timestamp" : "00:30.980-00:33.740",
    "text" : "Pour l'amour de Dieu"
  },
  {
    "text" : "Au clair de la lune",
